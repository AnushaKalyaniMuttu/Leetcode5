import java.util.*;

public class MaximumTransactionEachDay {

    public int[] maxDailyTransactions(int[] transactions, int[] dates) {
        int n = dates.length;
        int[] result = new int[n];

        Map<Integer, Integer> frequency = new HashMap<>();
        int maxSoFar = 0;

        for (int i = 0; i < n; i++) {
            int day = dates[i];

            frequency.put(day, frequency.getOrDefault(day, 0) + 1);
            maxSoFar = Math.max(maxSoFar, frequency.get(day));

            result[i] = maxSoFar;
        }

        return result;
    }
}
