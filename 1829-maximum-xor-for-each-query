public class Solution {

    public int[] getMaximumXor(int[] nums, int maximumBit) {
        int n = nums.length;
        int[] result = new int[n];

        int maxVal = (1 << maximumBit) - 1;

        // Compute XOR of entire array
        int prefixXor = 0;
        for (int num : nums) {
            prefixXor ^= num;
        }

        // Process queries in reverse
        for (int i = 0; i < n; i++) {
            result[i] = prefixXor ^ maxVal;   // best possible k
            prefixXor ^= nums[n - 1 - i];     // remove last element for next query
        }

        return result;
    }
}
