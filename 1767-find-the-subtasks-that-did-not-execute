WITH RECURSIVE nums AS (
  SELECT 1 AS n
  UNION ALL
  SELECT n + 1
  FROM nums
  WHERE n < (SELECT MAX(subtasks_count) FROM Tasks)
),
all_subtasks AS (
  SELECT t.task_id, nums.n AS subtask_id
  FROM Tasks t
  JOIN nums
    ON nums.n <= t.subtasks_count
)
SELECT a.task_id, a.subtask_id
FROM all_subtasks a
LEFT JOIN Executed e
  ON a.task_id = e.task_id
     AND a.subtask_id = e.subtask_id
WHERE e.subtask_id IS NULL;
