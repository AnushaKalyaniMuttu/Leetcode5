class Solution {
    public int[] decode(int[] encoded) {
        int n = encoded.length + 1;
        
        // Compute XOR of all numbers from 1 to n
        int totalXor = 0;
        for (int i = 1; i <= n; i++) {
            totalXor ^= i;
        }
        
        // XOR of encoded values at even indices
        int xorExceptLast = 0;
        for (int i = 0; i < encoded.length; i += 2) {
            xorExceptLast ^= encoded[i];
        }
        
        int[] perm = new int[n];
        // The last element
        perm[n - 1] = totalXor ^ xorExceptLast;
        
        // Reconstruct the rest backwards
        for (int i = n - 2; i >= 0; i--) {
            perm[i] = perm[i + 1] ^ encoded[i];
        }
        
        return perm;
    }
}
